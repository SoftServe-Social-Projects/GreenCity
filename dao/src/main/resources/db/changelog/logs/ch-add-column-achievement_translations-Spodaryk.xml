<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <changeSet id="Spodaryk-25" author="Oksana Spodaryk">
        <preConditions onFail="MARK_RAN">
            <not>
                <columnExists tableName="achievement_translations" columnName="name"/>
                <columnExists tableName="achievement_translations" columnName="name_eng"/>
            </not>
        </preConditions>
        <addColumn tableName="achievement_translations">
            <column name="name" type="varchar(255)" />
            <column name="name_eng" type="varchar(255)" />
        </addColumn>
    </changeSet>
</databaseChangeLog>
